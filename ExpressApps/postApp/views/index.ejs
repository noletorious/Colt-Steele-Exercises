<% include partials/header %>
<h1 class = "red">Just like the home page display all posts</h1>
<p>Generated with a for() loop</p>
    <% for(var i=0;i<posts.length;i++){ %>
            <li><%= posts[i].title %> - <strong><%= posts[i].author %></strong></li>
    <% }; %>

<p>Generated with a forEach() loop</p>

<% posts.forEach(function(post){ %>
    <li><%= post.title %> - <strong><%= post.author %></strong></li>
<% }) %>
<% include partials/footer %>